{
  "name": "@uncover/fundamentals-react",
  "version": "0.0.16",
  "author": "aSH <ash.uncover@gmail.com>",
  "description": "React implementation of ui5 fundamentals",
  "license": "MIT",
  "homepage": "https://ash-uncover.github.io/ap-fundamentals-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/ash-uncover/ap-fundamentals-react"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run build:tsc && npm run build:babel",
    "build:babel": "babel src/lib --extensions .ts,.tsx --out-dir dist --copy-files",
    "build:docs": "webpack --config webpack.config.docs.js",
    "build:tsc": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\" \"src/**/*.jsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "start": "webpack serve --progress --mode development",
    "start:docs": "webpack serve --config webpack.config.docs.js --progress --mode development",
    "test": "jest --env=jsdom",
    "test-coverage": "jest --env=jsdom --collectCoverage",
    "version": "npm run format"
  },
  "peerDependencies": {
    "@sap-theming/theming-base-content": "^11.1.44",
    "fundamental-styles": "^0.25.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@uncover/js-utils": "^0.0.9",
    "@uncover/js-utils-logger": "^0.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@reduxjs/toolkit": "^1.9.0",
    "@types/jest": "latest",
    "@types/node": "latest",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "i18next": "^22.0.5",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-chained-backend": "^4.0.1",
    "i18next-http-backend": "^2.0.2",
    "i18next-resources-to-backend": "^1.0.0",
    "jest": "latest",
    "jest-environment-jsdom": "latest",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "latest",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "style-loader": "^3.3.1",
    "ts-jest": "latest",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tslint": "latest",
    "tslint-config-prettier": "latest",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "node": true,
      "jest": true
    }
  }
}
